<div class="card-list-container py-5">
  <% @favorites.each do |favorite| %>
<div class="card-body">
      <div class="row">
        <div class="col-2">
          <%= image_tag favorite.user.get_profile_image, size: '50x50', class:"mt-3 rounded-circle"; %>
        </div>
        <div class="col-10">
          <p class="card-text"><%= link_to favorite_path(@favorite)do %><%= favorite.artist %><% end %></p>
        </div>
      </div>
    </div>
    <% end %>
</div>

